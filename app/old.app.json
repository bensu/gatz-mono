{
  "expo": {
    "name": "gatz.chat",
    "slug": "gatz",
    "version": "1.0.8",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "experiments": {
      "tsconfigPaths": true
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#3F4D34"
    },
    "assetBundlePatterns": [
      "**/*",
      "assets/**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "chat.gatz"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#3F4D34"
      },
      "package": "chat.gatz"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "scheme": "chat.gatz",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "be633365-719e-4fab-bfda-ced010b5613a"
      }
    },
    "owner": "sbensu"
  }
}
